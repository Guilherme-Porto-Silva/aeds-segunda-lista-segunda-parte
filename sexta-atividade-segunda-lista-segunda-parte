#include <stdio.h>

int main(void)
{
	char palavra[50];
	int i;
	printf("Digite uma palavra que tenha, no maximo, 50 caracteres\n");
	fgets(palavra, stdin);
	for(i=0; i<50; i++) palavra[i]++;
	fputs(palavra, sizeof(50), stdout);
	return 0;
}