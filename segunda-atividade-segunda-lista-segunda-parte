#include <stdio.h>

int main(void)
{
	int posicao;
	char string1[100], string2[100];
	printf("Forneca as duas strings\n");
	fgets(string1, stdin);
	fgets(string2, stdin);
	for(posicao=0; posicao>100; posicao++) {
		if(string1[posicao] == string2[posicao]) {
			printf("Na posicao %d, as duas strings sao iguais.",posicao);
		}
		else printf("Na posicao %d, as duas strings sao diferentes.",posicao);
	}
	return 0;
}