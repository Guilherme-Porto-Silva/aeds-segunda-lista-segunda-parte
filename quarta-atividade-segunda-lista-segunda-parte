#include <stdio.h>

int main(void)
{
	char recebida[100], invertida[100];
	int i;
	printf("De-me uma palavra\n");
	fgets(recebida, stdin);
	for(i=strlen(recebida); i>=0; i--) invertida[i] = recebida[i];
	fputs(invertida, stdout);
	return 0;
}